{
    "name": "diogodg/neoframework",
    "description": "NeoFramework simple and fast",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "NeoFramework\\Core\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\":"tests"
        }
    },
    "authors": [
        {
            "name": "Diogo Graciano",
            "email": "138625607+DiogoGraciano@users.noreply.github.com"
        }
    ],
    "bin": [
        "bin/neof"
    ],
    "minimum-stability": "stable",
    "require": {
        "phpmailer/phpmailer": "^6.9",
        "diogodg/neoorm": "^1.0",
        "league/flysystem": "^3.29",
        "php-di/php-di": "^7.0",
        "monolog/monolog": "^3.8",
        "matthiasmullie/minify": "^1.3",
        "filp/whoops": "^2.17",
        "nesbot/carbon": "^3.8",
        "guzzlehttp/guzzle": "^7.9",
        "respect/validation": "^2.4",
        "symfony/cache": "^7.2",
        "vlucas/phpdotenv": "^5.6",
        "adhocore/cli": "^1.9",
        "peppeocchi/php-cron-scheduler": "^4.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^7.2",
        "phpunit/phpunit": "^12.1"
    }
}
